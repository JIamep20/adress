  "scripts": {
    "debug": "node debug app.js",
    "start": "node app.js",
    "build-dev": "./node_modules/.bin/better-npm-run build-dev",
    "build-prod": "./node_modules/.bin/better-npm-run build-prod",
    "devs": "./node_modules/.bin/better-npm-run devs",
    "sails": "./node_modules/.bin/better-npm-run sails",
    "lift": "./node_modules/.bin/nodemon app.js --ignore web/"
  },
  "betterScripts": {
    "build-dev": {
      "command": "./node_modules/.bin/webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-prod": {
      "command": "./node_modules/.bin/webpack --stats --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "devs": {
      "command": "./node_modules/.bin/webpack-dev-server --config webpack.dev.server.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "sails": {
      "command": "./node_modules/.bin/webpack-dev-server --config webpack.sails.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "main": "app.js",
